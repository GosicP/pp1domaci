program TestIf

{

	int sum(int z, int d){
		return z + d;
	}	

	int inkrement(int a, int inc) 
		int pom; 
	{
		if(inc == 1) pom = 1;
		else if(inc == 2) pom = a;

		return sum(a,pom);
	}

	void main() int i, j; bool ibool;{
		
		i = 3;
		if (i == 3 && i != 3 || i >= 3) {
			print(1);
		}else{
			print(2);
		}
		
		ord('a');

	}

}