program TestIf

{

	int sum(int z, int d){
		return z + d;
	}	

	int inkrement(int a, int inc) 
		int pom; 
	{
		if(inc == 1) pom = 1;
		else if(inc == 2) pom = a;

		return sum(a,pom);
	}
	
	void mojaFunkc() {
		i = 3;
		if (i == 3 && i != 3 || i >= 3) {
			print(1);
		}else{
			print(2);
		}
		
	}

	void main() int i, j, niz[]; bool ibool;{

		niz = new int[3];
		niz[0] = 5;
		niz[1] = 6;
		niz[2] = 7;
		
		i = 0;
		
		do
			print(niz[i]);
		while (i < 3, i++);

	}

}